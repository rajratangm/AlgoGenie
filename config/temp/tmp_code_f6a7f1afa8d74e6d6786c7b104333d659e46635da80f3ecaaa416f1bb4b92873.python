def sort_list(input_list):
    # Return a sorted copy of the input list
    return sorted(input_list)

# Test cases
print(sort_list([3, 1, 4, 1, 5, 9, 2]))  # Test with integers
print(sort_list(['banana', 'apple', 'cherry']))  # Test with strings
print(sort_list([0.23, 0.1, 0.432]))  # Test with floats
